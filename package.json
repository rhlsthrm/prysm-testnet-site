    "core-js": "2.6.5",
    "deep-equal": "1.0.1",
    "ethers": "4.0.27",
    "express": "4.16.4",
    "google-protobuf": "3.7.0-rc.2",
    "grpc-web": "1.0.6",
    "ngx-captcha": "6.2.1",
    "node-sass": "4.11.0",
    "rxjs": "6.4.0",
