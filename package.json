{
  "name": "prysm-testnet-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gen-proto": "protoc -I=./src/proto ./src/proto/faucet.proto ./src/proto/validator.proto --js_out=import_style=commonjs:./src/proto --grpc-web_out=import_style=typescript,mode=grpcwebtext:./src/proto",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run prysm-testnet-site:server:production",
    "postinstall": "node patch.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.11",
    "@angular/cdk": "9.2.4",
    "@angular/common": "9.1.11",
    "@angular/compiler": "9.1.11",
    "@angular/core": "9.1.11",
    "@angular/forms": "9.1.11",
    "@angular/http": "7.2.16",
    "@angular/material": "9.2.4",
    "@angular/platform-browser": "9.1.11",
    "@angular/platform-browser-dynamic": "9.1.11",
    "@angular/platform-server": "9.1.11",
    "@angular/router": "9.1.11",
    "@nguniversal/express-engine": "9.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "@ngx-pwa/local-storage": "9.0.3",
    "@portis/web3": "2.0.0-beta.55",
    "@types/deep-equal": "1.0.1",
    "@walletconnect/web3-provider": "^1.0.7",
    "core-js": "3.6.5",
    "deep-equal": "2.0.3",
    "ethers": "5.0.2",
    "express": "4.17.1",
    "google-protobuf": "3.12.2",
    "grpc-web": "1.1.0",
    "ngx-captcha": "8.0.1",
    "node-sass": "4.14.1",
    "rxjs": "6.5.5",
    "tslib": "2.0.0",
    "web3": "^1.2.9",
    "websocket": "1.0.31",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.9",
    "@angular/cli": "9.1.9",
    "@angular/compiler-cli": "9.1.11",
    "@angular/language-service": "9.1.11",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^12.0.0",
    "codelyzer": "5.2.2",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "protractor": "7.0.0",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.8.3",
    "webpack-cli": "3.3.12"
  }
}
