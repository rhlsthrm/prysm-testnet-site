    "@angular/forms": "7.2.5",
    "@angular/http": "7.2.5",
    "@angular/material": "7.3.2",
    "@angular/platform-browser": "7.2.5",
    "@angular/platform-browser-dynamic": "7.2.5",
    "@angular/platform-server": "7.2.5",
    "@angular/router": "7.2.5",
    "@nguniversal/express-engine": "7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "@ngx-pwa/local-storage": "7.4.1",
    "@portis/web3": "2.0.0-beta.43",
    "@types/deep-equal": "1.0.1",
